
def pi(digits) {
    i = 0
    k = 0
    k1 = 1
    n=1
    a=0
    d=1
    t=0
    u=0
    run=1
    while 1 {
        k = k + 1
        t = n + n
        n = n * k
        a = a + t
        k1 = k1 + 2
        a = a * k1
        d = d * k1
        if a >= n {
            v = n*3+a
            t = v/d
            u = v%d
            u = u + n
            if d > u {
                putchar(t % 10 + 48)
                if (i == 0) {
                    putchar(46)
                }
                i = i + 1
                if i % 80 == 79 {
                    putchar(10)
                }
                if i >= digits {
                    putchar(10)
                    return 0
                }
                a = a - d*t
                a = a * 10
                n = n * 10
            }
        }
    }
}

pi(10000)
